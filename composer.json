{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "appstract/laravel-opcache": "4.0.2",
        "barryvdh/laravel-debugbar": "3.6.2",
        "codedge/laravel-fpdf": "1.7.2",
        "doctrine/dbal": "3.1.1",
        "fideloper/proxy": "4.4.1",
        "intervention/image": "2.6.1",
        "jeremykenedy/laravel-https": "1.4.0",
        "jeremykenedy/laravel-logger": "5.0.0",
        "jeremykenedy/laravel-roles": "6.0.0",
        "jeremykenedy/uuid": "3.1.0",
        "kouz/laravel-airbrake": "0.7.0",
        "laravel/framework": "8.57.0",
        "laravelcollective/html": "6.2.1",
        "rap2hpoutre/laravel-log-viewer": "1.7.0",
        "simplesoftwareio/simple-qrcode": "4.2.0"
    },
    "require-dev": {
        "filp/whoops": "2.14.0",
        "fzaninotto/faker": "1.9.2",
        "mockery/mockery": "1.4.3",
        "nunomaduro/collision": "5.9.0",
        "phpunit/phpunit": "9.5.8",
        "symfony/thanks": "1.2.10"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
